FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
    grad(p)         Gauss linear;
    grad(U)         Gauss linear;
    linExtrapGrad       Gauss linear;

}

divSchemes
{
    default                  none;
    div(tau)                 Gauss linear; 
    div(grad(U))             Gauss linear;
    div(phi,U)               GaussDefCmpw cubista; 
    div(phi,theta)           GaussDefCmpw cubista; 
    div(phi,tau)             GaussDefCmpw cubista;  
    div(eta*dev2(T(gradU)))  Gauss linear; 
    div((phi*rhoCp),T)       GaussDefCmpw cubista;  
}

laplacianSchemes
{
    default                       Gauss linear orthogonal;
     
}

interpolationSchemes
{
    default        linear;
      
}

snGradSchemes
{
    default         orthogonal;
}

fluxRequired
{
    default         no;
    p;
}

